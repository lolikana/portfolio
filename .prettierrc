{
	"singleQuote": true,
	"jsxSingleQuote": true,
	"arrowParens": "avoid",
	"printWidth": 90,
	"trailingComma": "es5",
	"semi": true,
	"useTabs": true,
	"tabWidth": 2,
	"bracketSpacing": true,
	"bracketSameLine": false,
	"endOfLine": "lf",
	"plugins": ["prettier-plugin-astro"],
	"overrides": [
		{
			"files": ["**/*.astro"],
			"options": {
				"parser": "astro"
			}
		}
	]
}
